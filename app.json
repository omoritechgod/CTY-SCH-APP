    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a7006434-b361-4e9d-919b-cf440aa18d63"
      }
    },
    "owner": "tguniverse",
    "android": {
      "package": "com.tguniverse.ctyschapp"
    }
  }
}
